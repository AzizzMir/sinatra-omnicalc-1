<form action="/random/results">
  <div>
    <label for="min_input">Minimum</label>
  
    <input type="text" name="user_min" id="min_input" placeholder="E.g. 1.5">
  </div>

  <div>
    <label for="max_input">Maximum</label>
  
    <input type="text" name="user_max" id="max_input" placeholder="E.g. 4.5">
  </div>

  <div>
    <button type="submit">Pick random number</button>
  </div>

</form>
